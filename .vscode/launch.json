    "version": "0.2.0",
    "configurations": [
        {
            "name": "agent-store: Debug Uvicorn",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "8002"
            ],
            "cwd": "${workspaceFolder}/agent-store",
            "jinja": true
        },
        {
            "name": "agent-engine: Debug Uvicorn",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "8001"
            ],
            "cwd": "${workspaceFolder}/agent-engine",
            "jinja": true
        }
    ]
}